{
  "version": 2,
  "name": "zhongmeng-daqui-bot",
  "builds": [
    {
      "src": "api/**/*.js",
      "use": "@vercel/node"
    },
    {
      "src": "*.html",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    },
    {
      "src": "/",
      "dest": "/index.html"
    },
    {
      "src": "/favicon.ico",
      "dest": "/404.html"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
